{
  "name": "nglink",
  "version": "6.0.0",
  "description": "Show geojson from url param",
  "author": "NextGIS",
  "private": true,
  "workspaces": [
    "server",
    "front"
  ],
  "scripts": {
    "dev:server": "bun run --filter nglink-server dev",
    "dev:front": "bun run --filter nglink-front dev",
    "prod": "bun run --filter nglink-front prod",
    "lint": "eslint src --ext .ts",
    "fix-lint": "eslint src --ext .ts --fix"
  },
  "devDependencies": {
    "bun-types": "^1.2.19",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2"
  },
  "license": "MIT"
}
